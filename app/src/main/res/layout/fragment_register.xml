                    android:background="@null"
                    android:gravity="end"
                    android:text=""
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="@color/item_group_edt"
                    android:textSize="13sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/item_group_divider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8sp"
                    android:layout_centerVertical="true"
                    android:textColor="@color/item_group_black"
                    android:textSize="15sp"
                    android:text="变更密码(pwd)" />

                <com.taide.ewarn.weight.ClearableEditText
                    android:drawablePadding="20dp"
                    android:id="@+id/Register_Edit_alterpwd"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="end"
                    android:text=""
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="@color/item_group_edt"
                    android:textSize="13sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/item_group_divider" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:orientation="horizontal">

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_register"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="终端&#10;注册"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_altertinfo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="变更&#10;信息"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_suspend"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="暂停"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_authorize"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="授权&#10;申请"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_getTokenInfo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="授权&#10;查询"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Register_Button_cancelAuth"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="授权&#10;撤销"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Button_saveInfo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="上传&#10;配置"
                    android:textSize="13dp" />

                <Button
                    style="@style/Button.Style"
                    android:background="@drawable/xui_config_bg_blue_btn"
                    android:layout_marginStart="5dp"
                    android:id="@+id/Button_startMqtt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="启动&#10;服务"
                    android:textSize="13dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:orientation="vertical">

                <com.xuexiang.xui.widget.textview.autofit.AutoFitTextView
                    android:id="@+id/Register_Edit_getinfo"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:maxLines="5"
                    android:text=""
                    android:textSize="20sp" />

                <com.xuexiang.xui.widget.textview.autofit.AutoFitTextView
                    android:id="@+id/Register_Edit_result"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:maxLines="5"
                    android:text=""
                    android:textSize="20sp" />

            </LinearLayout>
        </LinearLayout>

    </ScrollView>



</FrameLayout>